{
  "name": "ood-giving-campaign",
  "version": "0.0.2",
  "description": "Giving Campaign",
  "author": "Stanford University",
  "keywords": [
    "gatsby",
    "typescript",
    "react",
    "tailwindcss",
    "Stanford",
    "storyblok"
  ],
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "scripts": {
    "dev": "ENABLE_GATSBY_REFRESH_ENDPOINT=true gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "vault": "netlify-plugin-vault-variables",
    "vault:local": "NODE_ENV=development netlify-plugin-vault-variables",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --quiet --fix",
    "typecheck": "tsc --noEmit",
    "tsc": "tsc",
    "https-proxy-start": "local-ssl-proxy --source 3010 --target 8000 --cert ./dev/localhost.pem --key ./dev/localhost-key.pem",
    "hps": "local-ssl-proxy --cert ./dev/localhost.pem --key ./dev/localhost-key.pem"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "cnbuilder": "^3.1.0",
    "dotenv": "^16.0.3",
    "framer-motion": "^10.12.16",
    "gatsby": "^5.9.0",
    "gatsby-plugin-manifest": "^5.9.0",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sharp": "^5.9.0",
    "gatsby-plugin-sitemap": "^6.9.0",
    "gatsby-source-filesystem": "^5.9.0",
    "gatsby-source-storyblok": "^5.0.17",
    "gatsby-transformer-sharp": "^5.9.0",
    "netlify-plugin-vault-variables": "^1.0.7",
    "node-vault": "^0.9.22",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storyblok-rich-text-react-renderer-ts": "^3.2.0"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "autoprefixer": "^10.4.14",
    "decanter": "^7.0.0-rc.4",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gatsby-plugin-netlify": "^5.1.0",
    "gatsby-plugin-postcss": "^6.10.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.5"
  },
  "overrides": {
    "gatsby": "^5.9.0"
  },
  "engines": {
    "npm": ">=8.3",
    "node": ">=18"
  }
}
