{
  "name": "ood-giving-campaign",
  "version": "0.0.2",
  "description": "Giving Campaign",
  "author": "Stanford University",
  "keywords": [
    "nextjs",
    "typescript",
    "react",
    "tailwindcss",
    "Stanford",
    "storyblok"
  ],
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "hps": "local-ssl-proxy --cert ./dev/localhost.pem --key ./dev/localhost-key.pem",
    "https-proxy-start": "local-ssl-proxy --source 3010 --target 3000 --cert ./dev/localhost.pem --key ./dev/localhost-key.pem",
    "vault": "netlify-plugin-vault-variables",
    "vault:local": "NODE_ENV=development netlify-plugin-vault-variables",
    "lint:fix": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --quiet --fix",
    "typecheck": "tsc --noEmit",
    "tsc": "tsc"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@storyblok/react": "^2.4.0",
    "cnbuilder": "^3.1.0",
    "framer-motion": "^10.15.0",
    "next": "13.4.9",
    "storyblok-rich-text-react-renderer-ts": "^3.2.0"
  },
  "peerDependencies": {
    "react": "^18.2",
    "react-dom": "^18.2"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/node": "^20.4.7",
    "@types/react": "^18.2",
    "@types/react-dom": "^18.2",
    "autoprefixer": "^10.4.14",
    "decanter": "^7.1.1",
    "eslint": "^8.50.0",
    "eslint-config-next": "^13.4.13",
    "netlify-plugin-vault-variables": "^1.0.7",
    "postcss": "^8.4.27",
    "typescript": "^5.1"
  }
